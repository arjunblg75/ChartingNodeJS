    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Remote",
            "address": "https://chartingnodejs.azurewebsites.net",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "https://chartingnodejs.azurewebsites.net/site/wwwroot/"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\bin\\www"
        }
    ]
}